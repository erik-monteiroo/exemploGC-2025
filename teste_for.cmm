int i;
void main() {

   write("FOR COMPLETO");
    for (i = 0; i < 3; i = i + 1)
        write("i: ", i);

    write("FOR SEM INICIO");
    i = 0;
    for (; i < 2; i = i + 1)
        write("i: ", i);

    write("FOR SEM INCREMENTO");
    for (i = 0; i < 3; ) {
        write("i: ", i);
        i = i + 1;
    }
}
